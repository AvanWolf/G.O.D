<?xml version="1.0" ?>
<!DOCTYPE TranscendenceExtension
[
    <!ENTITY modGODCoreUseableHook      "0xDCBA0503">
    <!ENTITY stGODCorePlaceHook         "0xDCBA0504">

    <!ENTITY itGODCoreHook              "0xDCBA0502">
]>

<TranscendenceExtension UNID="&modGODCoreUseableHook;" version="1.0">

    <StationType UNID="&stGODCorePlaceHook;"
        backgroundObject=       "true"
        unique=                 "inSystem"
        virtual=                "true"
        >
        <Image imageID="&rsDebris1;" imageX="0" imageY="0" imageWidth="1" imageHeight="1"/>
        <Events>
          <OnGlobalPlayerEnteredSystem>
            (block Nil
                (if (not (objHasItem gPlayerShip (itmCreate &itGODCoreHook; 1)))
                    (objAddItem gPlayerShip (itmCreate &itGODCoreHook; 1))
                )
            )
          </OnGlobalPlayerEnteredSystem>
        </Events>
    </StationType>

</TranscendenceExtension>
